{
    "dataset":"Aidatatang-200zh",
    "src_spk":"G0002",
    "trg_spk":"G7072",
    "prefix":[5,10],
    "model":{
        "lambda_rec":10,
        "lambda_gp":10,
        "lambda_cls":10,
        "sampling_rate":48000
    },
    "train":{
        "batch_size":32,
        "num_iters":200000,
        "num_iters_decay":100000,
        "g_lr":0.0001,
        "d_lr":0.0001,
        "n_critic":5,
        "beta1":0.5,
        "beta2":0.999,
        "resume_iters":null
    },
    "test":{
        "test_iters":100000
    },
    "miscellaneous":{
        "num_workers":null,
        "mode":"train"
    },
    "directories":{
        "train_data_dir":"../../preprocessed_data/mel_train",
        "test_data_dir":"../../preprocessed_data/mel_test",
        "wav_dir":"../../preprocessed_data/mel_train",
        "log_dir":"./output/log",
        "model_save_dir":"./output/ckpt",
        "sample_dir":"./output/samples"
    },
    "step":{
        "log_step":10,
        "sample_step":10000,
        "model_save_step":1000,
        "lr_update_step":1000
    },
    "speaker_encoder":{
        "encoder":"lstm",
        "ckpt_path":"../../ckpt/se_step-3000000.pt",
        "config":{
            "model":{
                "model_hidden_size": 256,
                "model_embedding_size": 256,
                "model_num_layers": 3,
                "learning_rate_init": 1e-4,
                "speakers_per_batch": 64,
                "utterances_per_speaker": 10
            },
            "data":{
                "mel":{
                    "mel_window_length": 25,
                    "mel_window_step": 10,
                    "mel_n_channels": 40
                },
                "audio":{
                    "sampling_rate": 16000,
                    "partials_n_frames": 160,
                    "inference_n_frames": 80,
                    "audio_norm_target_dBFS": -30
                },
                "voice_activation_detection":{
                    "vad_window_length": 30,
                    "vad_moving_average_width": 8,
                    "vad_max_silence_length": 6
                }
            }
        }
    }
}